<%- include("partials/header") -%>
<h1>Compose</h1>
<form method="post" action="/compose">
    <div class="form-group">
    <div class="mb-3">
        <label for="postTitle" class="form-label">Title</label>
        <input id="postTitle" name="postTitle" type="text" class="form-control" id="exampleFormControlInput1">
      </div>
      <div class="mb-3">
        <label for="postBody" class="form-label">Post</label>
        <textarea class="form-control" id="postBody" rows="5" cols="30" name="postBody"></textarea>
      </div>
    </div>
      <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
